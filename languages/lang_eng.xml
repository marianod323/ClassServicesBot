<?xml version="1.0" encoding="UTF-8"?>
<language ref="english" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="lang.xsd">
	<command name="userla">
		<item type="title">Users last activity</item>
		<item type="help">Shows the users last activity on server</item>
		<item type="date">%m/%d/%Y, %H:%M</item>
		<item type="field1_name">User</item>
		<item type="field1_content">last activity on</item>
		<item type="field2_name">Page</item>
		<item type="field2_content">has no recent activity on text channel</item>
		<item type="field3_name">Help</item>
		<item type="field3_content">To navigate through pages, use **cs-userla _PAGE\_NUMBER_**</item>
	</command>
	<error name="userla_error">
		<item type="title">Oops...</item>
		<item type="field1_name">Error</item>
		<item type="field1_content">I've found an error on handling your request, maybe you forgot to specify the page number?</item>
		<item type="field2_name">Help</item>
		<item type="field2_content">To navigate through pages, use **cs-userla _PAGE\_NUMBER_**</item>
	</error>
	<command name="usercount">
		<item type="title">Users quantity</item>
		<item type="help">Shows how many users and bots are on the server</item>
		<item type="field1_name">Users</item>
		<item type="field2_name">Bots</item>
	</command>
	<command name="help">
		<item type="title">Help</item>
		<item type="help">Displays how to use each commands</item>
		<item type="prefix">cs-</item>
		<item type="field1_name">How to use commands</item>
		<item type="field1_content">To use bot commands, type the command in a text channel</item>
		<item type="field2_name">userla _PAGE\_NUMBER_</item>
		<item type="field3_name">usercount</item>
		<item type="field4_name">help</item>
		<item type="field5_name">setlang _TAG_</item>
		<item type="field6_name">settz _VALUE_</item>
		<item type="field7_name">setname _"NAME\_BETWEEN\_QUOTE\_MARKS"_</item>
		<item type="field8_name">getnames</item>
		<item type="field9_name">setrole _"VALUE\_BETWEEN\_QUOTE\_MARKS"_</item>
	</command>
	<command name="setlang">
		<item type="title">Language</item>
		<item type="help">Defines bot's language</item>
		<item type="success">Language modified!</item>
		<item type="success_title">Success!</item>
		<item type="fail">The language you are trying to enter is not listed!</item>
		<item type="fail_title">Language not modified!</item>
	</command>
	<command name="settz">
		<item type="title">Time Zone</item>
		<item type="help">Defines bot's time zone</item>
		<item type="success">Time Zone changed!</item>
		<item type="success_title">Success!</item>
	</command>
	<command name="setname">
		<item type="title">Names</item>
		<item type="help">Creates a relation between the user's username and real name</item>
		<item type="success">Name changed!</item>
		<item type="success_title">Success!</item>
	</command>
	<command name="getnames">
		<item type="title">Names</item>
		<item type="help">Shows the relation between the user's username and real name</item>
		<item type="users">Users in server</item>
	</command>
	<command name="setrole">
		<item type="title">Restrictions</item>
		<item type="help">Defines the power level needed to use sensitive commands</item>
		<item type="fail_title">Fail</item>
		<item type="fail">You don't have permission to do this.</item>
		<item type="fail_2">The role you are trying to define does not exists.</item>
		<item type="success_title">Success</item>
		<item type="success">Role for using the sensitive commands redefined!</item>
	</command>
</language>
