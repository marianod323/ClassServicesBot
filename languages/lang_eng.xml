<?xml version="1.0" encoding="UTF-8"?>
<language ref="english" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="lang.xsd">
	<command name="userla">
		<item type="title">Users last activity</item>
		<item type="help">Shows the users last activity on server</item>
		<item type="date">%m/%d/%Y, %H:%M</item>
		<item type="field1_name">User</item>
		<item type="field1_content">last activity on</item>
		<item type="field2_name">Page</item>
		<item type="field2_content">has no recent activity on text channel</item>
		<item type="field3_name">Help</item>
		<item type="field3_content">To navigate through pages, use **cs-userla _PAGE\_NUMBER_**</item>
	</command>
	<error name="userla_error">
		<item type="title">Oops...</item>
		<item type="field1_name">Error</item>
		<item type="field1_content">I've found an error on handling your request, maybe you forgot to specify the page number?</item>
		<item type="field2_name">Help</item>
		<item type="field2_content">To navigate through pages, use **cs-userla _PAGE\_NUMBER_**</item>
	</error>
	<command name="usercount">
		<item type="title">Users quantity</item>
		<item type="help">Shows how many users and bots are on the server</item>
		<item type="field1_name">Users</item>
		<item type="field2_name">Bots</item>
	</command>
	<command name="help">
		<item type="title">Help</item>
		<item type="help">Displays how to use each commands</item>
		<item type="prefix">cs-</item>
		<item type="field1_name">How to use commands</item>
		<item type="field1_content">To use bot commands, type the command in a text channel</item>
		<item type="field2_name">userla _PAGE\_NUMBER_</item>
		<item type="field3_name">usercount</item>
		<item type="field4_name">help</item>
	</command>
</language>
